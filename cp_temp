//#define FILEIO
#include<bits/stdc++.h>

using namespace std;

const int64_t MODVAL = 1e9 + 7;

int64_t power(int64_t x, int64_t n, int64_t mod){
	if(n == 0){
		return 1;
	}
	else if(n % 2){
		return (x * power(x, n - 1, mod)) % mod; 
	}
	int64_t temp = power(x, n / 2, mod);
	return (temp * temp) % mod;
}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	#ifdef FILEIO
	freopen("in", "r", stdin);
	freopen("out", "w", stdout);
	#endif
	return 0;
}
